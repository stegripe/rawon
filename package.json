{
  "name": "disc-11",
  "version": "2.0.0-dev",
  "private": true,
  "description": "A dedicated open-source Discord bot for Zhycorp based from our Discord bot template with more features. Easy to use, and with no coding required.",
  "repository": "https://github.com/zhycorp/disc-11",
  "license": "BSD-3-Clause",
  "author": "Zhycorp <support@zhycorp.net>",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run compile",
    "compile": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start": "node .",
    "start:dev": "ts-node -r dotenv/config src/bot.ts"
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@zhycorp/eslint-config/typescript"
  },
  "dependencies": {
    "@discordjs/voice": "^0.6.0",
    "date-fns": "^2.23.0",
    "discord.js": "^13.1.0",
    "entities": "^3.0.1",
    "ffmpeg-static": "^4.4.0",
    "got": "^11.8.2",
    "soundcloud.ts": "^0.4.1",
    "spotify-url-info": "^2.2.3",
    "tslib": "^2.3.1",
    "winston": "^3.3.3",
    "youtube-dl-exec": "^1.2.6",
    "youtubei": "^0.0.1-rc.23"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "@zhycorp/eslint-config": "^4.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "optionalDependencies": {
    "@discordjs/opus": "^0.6.0",
    "bufferutil": "^4.0.3",
    "libsodium-wrappers": "^0.7.9",
    "utf-8-validate": "^5.0.6",
    "zlib-sync": "^0.1.7"
  },
  "engines": {
    "node": ">=16.6.0"
  }
}
