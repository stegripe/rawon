{
    "name": "rawon",
    "version": "4.5.5",
    "description": "A simple powerful Discord music (multi-)bot built to fulfill your production desires. Easy to use, with no coding required.",
    "license": "AGPL-3.0",
    "author": "Stegripe Development <support@stegripe.org>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/stegripe/rawon.git"
    },
    "bugs": {
        "url": "https://github.com/stegripe/rawon/issues"
    },
    "homepage": "https://github.com/stegripe/rawon#readme",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "pnpm run lint && pnpm run compile",
        "compile": "npx swc src -d dist --strip-leading-paths",
        "tscompile": "tsc --build tsconfig.json",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "start": "node --es-module-specifier-resolution=node -r dotenv/config . | pino-pretty -t \"SYS:yyyy-MM-dd HH:mm:ss\"",
        "dev": "rimraf ./dist && pnpm run compile && pnpm start"
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=7.0.0"
    },
    "dependencies": {
        "@discordjs/voice": "^0.19.0",
        "@sapphire/decorators": "^6.2.0",
        "@sapphire/framework": "^5.5.0",
        "@sapphire/utilities": "^3.18.2",
        "@stegripe/command-context": "^1.2.0",
        "@stegripe/pino-logger": "^1.2.0",
        "@swc/cli": "0.8.0",
        "@swc/core": "1.15.13",
        "@types/better-sqlite3": "^7.6.13",
        "better-sqlite3": "^12.6.2",
        "date-fns": "4.1.0",
        "discord.js": "14.25.1",
        "dotenv": "17.3.1",
        "ffmpeg-static": "^5.3.0",
        "got": "14.6.6",
        "i18n": "0.15.3",
        "opusscript": "0.0.8",
        "pino": "^10.3.1",
        "pino-pretty": "^13.1.3",
        "prism-media": "1.3.5",
        "puppeteer": "^24.37.5",
        "puppeteer-core": "^24.37.5",
        "@puppeteer/browsers": "^2.13.0",
        "soundcloud.ts": "^0.7.2",
        "tslib": "2.8.1",
        "tweetnacl": "1.0.3",
        "youtubei": "1.8.4",
        "zip-lib": "^1.2.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.4.4",
        "@stegripe/biomejs-config": "^1.1.0",
        "@types/i18n": "0.13.12",
        "@types/node": "24.10.13",
        "rimraf": "^6.1.3",
        "typescript": "5.9.3"
    },
    "optionalDependencies": {
        "@snazzah/davey": "^0.1.9"
    }
}
